<% include ../partials/header %>
	
	<div class="no-match-err"><!-- error for search spots -->
        <% if(noMatch !== null){ %>
          <div>
            <p class="search-alert"><%= noMatch %></p>
          </div>
          
        <% } %>
    </div>

	<div class="container">
		<% parks.forEach(function(park){ %>				
 			<div class="thumbnail">
				<img src= "<%= park.image %>">
				<div class="caption">
					<h3><%= park.name %></h3>
					<h4 class="sub-title"><%= park.location %></h4>
					<% if (park.rating === 0) { %>
                	<em>No reviews yet.</em>
                	<% } else { %>
                	<span class="fa fa-star checked"></span>
                	<span class="fa fa-star <% if (park.rating > 1.5) { %> checked <% } %>"></span>
                	<span class="fa fa-star <% if (park.rating > 2.5) { %> checked <% } %>"></span>
                	<span class="fa fa-star <% if (park.rating > 3.5) { %> checked <% } %>"></span>
                	<span class="fa fa-star <% if (park.rating > 4.5) { %> checked <% } %>"></span>
                	<% } %>
                	
				</div>
				<a href="/parks/<%= park._id %>" class="index-btn">More Info</a>																		
			</div>			
		<% }); %>
	</div>		


<% include ../partials/footer %>